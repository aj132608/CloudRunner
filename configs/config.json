{
  "compute": ["local", "AWS", "GCloud"],
  "local": {
      "CPU": 6,
      "RAM": 16,
      "queue_type": "local"
  },
  "AWS": {
      "cpus": 6,
      "ram": 16,
      "gpus": 2,
      "num_workers": 2,
      "env": {
          "cdsm_aws_access_key": "AWS_ACCESS_KEY_ID",
          "cdsm_aws_secret_key": "AWS_SECRET_KEY_ID"
      },
    "image": ""
  },
  "GCloud": {
      "resources": {
          "cpu": 6,
          "ram": 16,
          "gpu_type": "nvidia-tesla-k80",
          "num_of_gpu": 2,
          "disk_type": "pd-ssd",
          "disk_space": 60,
          "num_workers": 2
      },
      "env": {
          "cdsm_google_cred_path": "GCLOUD_CREDENTIAL_PATH"
      }
  },
  "storage": {
        "type": "minio",
        "endpoint_url": "http://localhost:9000",
        "env": {
          "cdsm_aws_access_key": "AWS_ACCESS_KEY_ID",
          "cdsm_aws_secret_key": "AWS_SECRET_KEY_ID"
      }
    },
    "queue": {
        "type": "rmq",
        "queue_name": "test_queue",
        "exchange_name": "jobs",
        "endpoint": "amqp://guest:guest@localhost"
    }
}
